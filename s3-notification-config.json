{
  "TopicConfigurations": [
    {
      "TopicArn": "arn:aws:sns:us-east-1:<AWS_ACCOUNT_ID>:image-processing-topic",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "resize/"
            }
          ]
        }
      }
    },
    {
      "TopicArn": "arn:aws:sns:us-east-1:<AWS_ACCOUNT_ID>:image-processing-topic",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "greyscale/"
            }
          ]
        }
      }
    },
    {
      "TopicArn": "arn:aws:sns:us-east-1:<AWS_ACCOUNT_ID>:image-processing-topic",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "exif/"
            }
          ]
        }
      }
    }
  ]
}